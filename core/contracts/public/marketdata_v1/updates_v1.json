{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "marketdata.updates.v1",
    "title": "MarketData Updates v1",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "ok",
        "symbol",
        "tf_s",
        "events",
        "cursor_seq",
        "boot_id"
    ],
    "properties": {
        "ok": {
            "const": true
        },
        "symbol": {
            "type": "string"
        },
        "tf_s": {
            "type": "integer"
        },
        "events": {
            "type": "array",
            "items": {
                "$ref": "#/$defs/event"
            }
        },
        "cursor_seq": {
            "type": "integer",
            "minimum": 0
        },
        "boot_id": {
            "type": "string"
        },
        "disk_last_open_ms": {
            "type": "integer"
        },
        "bar_close_ms": {
            "type": "integer"
        },
        "ssot_write_ts_ms": {
            "type": "integer"
        },
        "api_seen_ts_ms": {
            "type": "integer"
        },
        "warnings": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "$defs": {
        "event": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "key",
                "bar",
                "complete",
                "source"
            ],
            "properties": {
                "key": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": [
                        "symbol",
                        "tf_s",
                        "open_ms"
                    ],
                    "properties": {
                        "symbol": {
                            "type": "string"
                        },
                        "tf_s": {
                            "type": "integer"
                        },
                        "open_ms": {
                            "type": "integer"
                        }
                    }
                },
                "bar": {
                    "$ref": "bar_v1.json"
                },
                "complete": {
                    "type": "boolean"
                },
                "source": {
                    "type": "string"
                },
                "event_ts": {
                    "type": [
                        "integer",
                        "null"
                    ]
                }
            }
        }
    }
}